{
  "name": "devrant-menu-bar-app",
  "version": "0.0.1",
  "description": "A simple menu bar app to read rants.",
  "main": "src/js/main.js",
  "scripts": {
    "test": "mocha --require babel-core/register",
    "lint": "eslint --color src/ test/ --ext .js*",
    "clean": "rimraf dist coverage",
    "build:webpack": "webpack --colors --progress -d",
	"build:webpack-dev": "npm run build:webpack -- --watch",
    "build:electron": "NODE_ENV=development electron src/js/main.js",
    "start": "npm run clean && npm run build:webpack && npm run build:electron"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danillouz/devrant-menu-bar-app.git"
  },
  "keywords": [
    "devrant",
    "menu-bar-app",
    "electron",
    "react",
    "redux"
  ],
  "author": "Daniel Illouz",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/danillouz/devrant-menu-bar-app/issues"
  },
  "homepage": "https://github.com/danillouz/devrant-menu-bar-app#readme",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "devtron": "^1.1.2",
    "electron-prebuilt": "^1.2.0",
    "enzyme": "^2.3.0",
    "eslint": "^2.11.1",
    "eslint-plugin-react": "^5.1.1",
    "mocha": "^2.5.3",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1"
  },
  "dependencies": {
    "menubar": "^4.1.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0"
  }
}
