{
  "name": "devrant-menu-bar-app",
  "version": "0.0.2",
  "description": "A simple menu bar app to read rants.",
  "main": "src/js/main.js",
  "scripts": {
    "test": "mocha --require babel-core/register",
    "lint": "eslint --color src/ test/ --ext .js*",
    "clean": "rimraf dist coverage",
    "build:hot": "NODE_ENV=development npm run clean && node dev.server.js",
    "build:webpack": "NODE_ENV=production webpack --progress --config webpack.config.prod.js",
    "build:electron-dev": "NODE_ENV=development electron src/js/main.js",
    "start": "npm run clean && npm run build:webpack && npm run build:electron-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danillouz/devrant-menu-bar-app.git"
  },
  "keywords": [
    "devrant",
    "menu-bar-app",
    "electron",
    "react",
    "redux"
  ],
  "author": "Daniel Illouz",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/danillouz/devrant-menu-bar-app/issues"
  },
  "homepage": "https://github.com/danillouz/devrant-menu-bar-app#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-react-display-name": "^6.8.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "devtron": "^1.4.0",
    "electron": "^1.4.13",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "material-ui": "^0.16.6",
    "menubar": "^5.2.0",
    "mocha": "^3.2.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.3",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "sinon": "^1.17.7",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-hot-middleware": "^2.15.0"
  }
}
